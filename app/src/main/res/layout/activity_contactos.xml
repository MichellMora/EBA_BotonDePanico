<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/fondo_azul"
    tools:context=".contactos">

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Space
            android:layout_width="match_parent"
            android:layout_height="30dp" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/contNom1"
                    android:layout_width="210dp"
                    android:layout_height="48dp"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:textSize="20dp" />

            </LinearLayout>

            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/btnListo1"
                android:layout_width="38dp"
                android:layout_height="34dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:backgroundTint="@color/boton_azul"
                android:clickable="true"
                android:src="@drawable/ic_baseline_check_24"
                app:fabCustomSize="34dp"
                app:maxImageSize="34dp" />

            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/elm1"
                android:layout_width="38dp"
                android:layout_height="34dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:backgroundTint="@color/gris_claro"
                android:clickable="true"
                app:fabCustomSize="34dp"
                app:maxImageSize="34dp"
                app:srcCompat="@drawable/ic_baseline_delete_24" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="208dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/contNom2"
                    android:layout_width="match_parent"
                    android:layout_height="48dp"
                    android:gravity="center"
                    android:textSize="20dp" />

            </LinearLayout>

            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/btnListo2"
                android:layout_width="38dp"
                android:layout_height="34dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:backgroundTint="@color/boton_azul"
                android:clickable="true"
                android:src="@drawable/ic_baseline_check_24"
                app:fabCustomSize="34dp"
                app:maxImageSize="34dp" />

            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/elm2"
                android:layout_width="38dp"
                android:layout_height="34dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:backgroundTint="@color/gris_claro"
                android:clickable="true"
                app:fabCustomSize="34dp"
                app:maxImageSize="34dp"
                app:srcCompat="@drawable/ic_baseline_delete_24" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="210dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/contNom3"
                    android:layout_width="match_parent"
                    android:layout_height="48dp"
                    android:gravity="center"
                    android:textSize="20dp" />

            </LinearLayout>

            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/btnListo3"
                android:layout_width="38dp"
                android:layout_height="34dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:backgroundTint="@color/boton_azul"
                android:clickable="true"
                android:src="@drawable/ic_baseline_check_24"
                app:fabCustomSize="34dp"
                app:maxImageSize="34dp" />

            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/elm3"
                android:layout_width="38dp"
                android:layout_height="34dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:backgroundTint="@color/gris_claro"
                android:clickable="true"
                app:fabCustomSize="34dp"
                app:maxImageSize="34dp"
                app:srcCompat="@drawable/ic_baseline_delete_24" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="210dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/contNom4"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:gravity="center"
                    android:textSize="20dp" />

            </LinearLayout>

            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/btnListo4"
                android:layout_width="38dp"
                android:layout_height="34dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:backgroundTint="@color/boton_azul"
                android:clickable="true"
                android:src="@drawable/ic_baseline_check_24"
                app:fabCustomSize="34dp"
                app:maxImageSize="34dp" />

            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/elm4"
                android:layout_width="38dp"
                android:layout_height="34dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:backgroundTint="@color/gris_claro"
                android:clickable="true"
                app:fabCustomSize="34dp"
                app:maxImageSize="34dp"
                app:srcCompat="@drawable/ic_baseline_delete_24" />

        </LinearLayout>

        <Space
            android:layout_width="match_parent"
            android:layout_height="30dp" />

        <EditText
            android:id="@+id/etNombreCont"
            android:layout_width="322dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:background="@drawable/btn_transparenteazul"
            android:gravity="center"
            android:singleLine="true"
            android:hint="NOMBRE"
            android:textSize="20dp" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="30dp" />

        <EditText
            android:id="@+id/etTel"
            android:layout_width="322dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:background="@drawable/btn_transparenteazul"
            android:gravity="center"
            android:singleLine="true"
            android:hint="TELÃ‰FONO"
            android:textSize="20dp" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="30dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="59dp"
            android:orientation="horizontal">

            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/btnCont1"
                android:layout_width="62dp"
                android:layout_height="34dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:backgroundTint="@color/gris_claro"
                android:clickable="true"
                android:src="@drawable/ic_baseline_add_24"
                app:fabCustomSize="34dp"
                app:maxImageSize="34dp" />

            <Button
                android:id="@+id/btnVolver"
                style="?attr/materialButtonOutlinedStyle"
                android:layout_width="267dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:backgroundTint="@color/boton_azul"
                android:gravity="center"
                android:text="Guardar"
                app:strokeColor="@color/actionbar_azul" />

        </LinearLayout>
    </LinearLayout>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:text="Contactos"
        android:textColor="@color/actionbar_azul"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>